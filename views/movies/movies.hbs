<h2>Movies</h2>
{{#each movies}}
    <a href="/movies/{{this._id}}">{{this.title}}</a>
    <div>
        <a href="/movies/{{this._id}}/edit" class="btn btn-dark">Edit</a>
        <form action="/movies/{{this._id}}/delete" method="post">
        <button type="submit" class="btn btn-warning">Delete</button>
        </form>
    </div>
{{/each}}